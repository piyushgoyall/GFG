//{ Driver Code Starts
#include <bits/stdc++.h>
using namespace std;

// } Driver Code Ends
class Solution 
{
  public:
    int cppIntType() 
    {
        // code here
        int c;
        cin>>c;
        return c;
    }
    
    char cppCharType() 
    {
        // code here
        char d;
        cin>>d;
        return d;
    }
    
    float cppFloatType() 
    {
        // code here
        float e;
        cin>>e;
        return e;
    }
};

//{ Driver Code Starts.
int main() {
    int t;
    cin >> t;
    while (t--) {
        Solution ob;
        cout << ob.cppIntType() << endl;
        cout << ob.cppCharType() << endl;
        cout << ob.cppFloatType() << endl;
    }
    return 0;
}
// } Driver Code Ends
